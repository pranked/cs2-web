{
  "name": "web",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "rimraf ./src/build && concurrently --kill-others \"npm run dev-server\" \"npm run dev-client\"",
    "dev-server": "node app.js",
    "dev-client": "node webpack.js",
    "build": "rimraf ./src/build && NODE_ENV=production webpack --config ./webpack.config.js --progress --profile --colors --bail",
    "lint": "eslint src"
  },
  "dependencies": {
    "axios": "^0.10.0",
    "bourbon": "^4.2.7",
    "classnames": "^2.2.5",
    "express": "^4.13.4",
    "history": "^2.1.0",
    "jquery": "^3.1.0",
    "lodash": "^4.11.1",
    "react": "^15.0.1",
    "react-bootstrap": "^0.30.3",
    "react-document-title": "^2.0.2",
    "react-dom": "^15.0.1",
    "react-router": "^2.8.1",
    "react-side-effect": "^1.0.2",
    "scroll-behavior": "^0.4.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "concurrently": "^2.0.0",
